<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gwadaboyz.lokacar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MenuActivity"/>

        <activity android:name=".activity.VehiculeActivity" android:parentActivityName=".activity.MenuActivity"/>
        <activity android:name=".activity.LocationActivity" android:parentActivityName=".activity.MenuActivity"/>
        <activity android:name=".activity.ClientActivity" android:parentActivityName=".activity.MenuActivity"/>
        <activity android:name=".activity.CAActivity" android:parentActivityName=".activity.MenuActivity"/>
    </application>

</manifest>